<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.taskplanner.view.listview.ListViewController">
<children>
      <ListView fx:id="listOfNotes" layoutX="651.0" prefHeight="548.0" prefWidth="348.0" />
      <Button fx:id="newNoteButton" layoutX="52.0" layoutY="68.0" mnemonicParsing="false" onMouseClicked="#createNewNote" prefHeight="27.0" prefWidth="76.0" text="Add" />
      <Button fx:id="removeButton" layoutX="554.0" layoutY="87.0" mnemonicParsing="false" onMouseClicked="#deleteSelectedNote" prefHeight="27.0" prefWidth="76.0" text="Remove" />
      <TextField fx:id="newTitle" layoutX="47.0" layoutY="28.0" promptText="New note title" />
      <Button fx:id="openNoteButton" layoutX="554.0" layoutY="41.0" mnemonicParsing="false" onMouseClicked="#openSelectedNote" prefHeight="27.0" prefWidth="76.0" text="Open" />
      <Button fx:id="plusButton" layoutX="52.0" layoutY="176.0" mnemonicParsing="false" onAction="#createAndOpenNote" />
      <Label layoutX="52.0" layoutY="147.0" text="Add empty note:" />
</children>
</AnchorPane>
